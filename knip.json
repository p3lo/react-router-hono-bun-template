{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": ["scripts/*.{ts,js}", "app/routes.ts", "app/server/*.ts"],
	"lefthook": true,
	"remix": true,
	"project": ["**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx}"],
	"ignore": [
		"app/library/icon/icons/types.ts",
		"react-router.config.ts",
		"app/library/icon/Icon.tsx",
		"app/library/link/link.tsx",
		"app/localization/resource.ts",
		"app/lib/utils.ts",
		"app/components/**/*"
	],
	"ignoreDependencies": [
		"@babel/preset-typescript",
		"babel-plugin-react-compiler",
		"tailwindcss",
		"class-variance-authority",
		"lucide-react",
		"tailwindcss-animate",
		"tw-animate-css",
		"clsx",
		"tailwind-merge"
	]
}
